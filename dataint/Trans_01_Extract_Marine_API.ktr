    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// 1. Conversão da velocidade da corrente (m/s para km/h)
// Se o valor de entrada for nulo ou inválido, o resultado será nulo.
var ocean_current_kmh = (ocean_current_velocity != null) ? ocean_current_velocity * 3.6 : null;

// 2. Conversão da temperatura (Celsius para Fahrenheit)
var sea_temp_fahrenheit = (sea_surface_temperature != null) ? (sea_surface_temperature * 9/5) + 32 : null;

// 3. Cálculo da energia da onda (em kJ/m²)
// Fórmula: E = (1/2) * ρ * g * H²
// ρ (densidade da água do mar) ≈ 1025 kg/m³
// g (aceleração da gravidade) ≈ 9.81 m/s²
// O resultado é dividido por 1000 para converter de Joules para kilojoules (kJ)
var wave_energy_kj_m2 = (wave_height != null &amp;&amp; wave_height >= 0) ? (0.5 * 1025 * 9.81 * Math.pow(wave_height, 2)) / 1000 : null;

</jsScript_script>
      </jsScript>
    </jsScripts>
